#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/config/workspace/1770355441777-68a9b2648cd1ba15f2ff2bbc/node_modules/.pnpm/json5@2.2.3/node_modules/json5/lib/node_modules:/config/workspace/1770355441777-68a9b2648cd1ba15f2ff2bbc/node_modules/.pnpm/json5@2.2.3/node_modules/json5/node_modules:/config/workspace/1770355441777-68a9b2648cd1ba15f2ff2bbc/node_modules/.pnpm/json5@2.2.3/node_modules:/config/workspace/1770355441777-68a9b2648cd1ba15f2ff2bbc/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/config/workspace/1770355441777-68a9b2648cd1ba15f2ff2bbc/node_modules/.pnpm/json5@2.2.3/node_modules/json5/lib/node_modules:/config/workspace/1770355441777-68a9b2648cd1ba15f2ff2bbc/node_modules/.pnpm/json5@2.2.3/node_modules/json5/node_modules:/config/workspace/1770355441777-68a9b2648cd1ba15f2ff2bbc/node_modules/.pnpm/json5@2.2.3/node_modules:/config/workspace/1770355441777-68a9b2648cd1ba15f2ff2bbc/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../json5/lib/cli.js" "$@"
else
  exec node  "$basedir/../json5/lib/cli.js" "$@"
fi
